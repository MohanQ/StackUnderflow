@model List<QuestionsAndAnswers.ControllersModel.UserListModel>

@{
    ViewBag.Title = Resources.Global.Users;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Resources.Global.Users</h2>
@foreach (var item in Model)
{
    <p><b>@Html.ActionLink(item.User.UserName, "GetUser", "Home", new {id=item.User.UserId}, null)</b> - @Resources.Global.Reputation: @item.Rating</p>
}

@section scripts{
    <script type="text/javascript">
        $(function () {
            $("#allusersurl").addClass("active");
        });
    </script>
}