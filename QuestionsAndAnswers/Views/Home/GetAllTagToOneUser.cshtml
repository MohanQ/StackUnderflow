@model IEnumerable<QuestionsAndAnswers.Models.Tag>

@{
    ViewBag.Title = "GetAllTagToOneUser";
}

<h2>@Resources.Global.GetAllTagToOneUserHead @ViewBag.UserName </h2>

<table>
    <tr>
        <th>
            @Resources.Global.Tags
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink(Resources.Global.Profil, "Tag", new { id=item.Id }) 
           
        </td>
    </tr>
}

</table>
